{
    "manifest_version": 3,
    "name": "Lead Tracker",
    "version": "1.0.0",
    "description": "A simple extension to save, tag, and manage URLs from tabs or manual input.",
    "permissions": [
        "tabs",
        "storage"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icon16.png",
            "48": "icon48.png",
            "128": "icon128.png"
        }
    }
}